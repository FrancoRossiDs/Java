Algoritmo imprimirNombres
	Definir nombre Como Caracter;
	Definir i Como Entero;
	
	Para i=0 Hasta 10 Hacer
		Imprimir "Ingrese un nombre";
		Leer nombre;
		saludo(nombre);
		Imprimir "***********************************************************";
	FinPara
FinAlgoritmo
Funcion saludo(nombrE Por Referencia) 
	Imprimir "Bienvenido ", nombrE;
FinFuncion
